        this.userService.redirectURL = url;
        if (!this.userService.isLoggedIn()) {
            this.router.navigate(['/login']);
            return false;
        }
        else if (!this.userService.isAdmin()) {
            this.router.navigate(['/401']);
            return false;
        }
        return true;
    };
